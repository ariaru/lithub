<p id="notice"><%= notice %></p>

<h1>Documents on Branch <%= @branch.name %></h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <%= render 'documents/docs_list', document: document %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document', new_branch_document_path %>